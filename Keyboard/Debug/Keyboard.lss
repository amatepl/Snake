                                 ;Add food for snake on the screen
                                 generate :
000128 7f7f                      	ANDI random,0b011111111         ; We want a random number < 70 => we need 7 bits => random,7 = 0
000129 3476                      	CPI random,70                   ; Compare random register to 70
00012a f408                      	BRSH LSFR270                    ; If it's equal or bigger than 70 use LSFR
00012b c00a                      	RJMP food                       
                                 LSFR270 :
00012c 2f07                      	MOV R16, random                 ; Clone random to R16                    ex : R16 = 0b01111110
00012d 2f37                      	MOV R19, random                 ; Clone random to R19                         R17 = 0b01111110
00012e 9576                      	LSR random                      ; Shift to the right random                   random = 0b00111111
00012f fb00                      	BST R16,0					    ; Take the first bit of R16                   T = 0
000130 f976                      	BLD random,6                    ; Put this at seventh place of random         random = 0b00111111
000131 f936                      	BLD R19,6                       ; Same for R19                                R17 = 0b00111110
000132 2730                      	EOR R19,R16                     ; R19 = R16 xor R19                           R17 = 0b01000000
000133 fb36                      	BST R19,6                       ; Take the seventh bit of R19                 T = 1
000134 f975                      	BLD random,5                    ; Put this at the sixth place of random       random = 0b00111111
000135 cff2                      	RJMP generate                   ; Test if the new random is smaller than 70
                                 
